@model List<Practice.Models.Movie>
@{
    ViewBag.Title = "Index";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <h1>List of Movies</h1>
            <hr />
        </div>
    </div>
    
    <div class="row">
        <div class="col-md-4">
            <h4>Name</h4>
        </div>
        <div class="col-md-4">
            <h4>Genre</h4>
        </div>
        <div class="col-md-4">
            <h4>Price</h4>
        </div>
    </div>
</div>
@foreach (var item in Model)
{
    @Html.Partial("DisplayMovieCollection", item)
}

